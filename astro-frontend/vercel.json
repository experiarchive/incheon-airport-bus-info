{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "astro",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://air.koreabusinfo.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "incheon-airport-bus-info-bjkt.vercel.app"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
} 